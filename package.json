{
  "name": "neo4j-tutorial",
  "version": "0.0.0",
  "private": true,
  "description": "A node web application using neo4j",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development DEBUG=neo4j-tutorial:* nodemon ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "NODE_ENV=test mocha --opts tests/unit/mocha.opts tests/unit/**/*.js",
    "test-integration": "NODE_ENV=test mocha --opts tests/integration/mocha.opts tests/integration/**/*.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "neo4j-driver": "^1.2.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "supertest": "^3.0.0",
    "testdouble": "^2.1.2"
  }
}
